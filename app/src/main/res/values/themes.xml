<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.ServiceXpress" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/prussian_blue</item>
        <item name="colorPrimaryVariant">@color/prussian_blue</item>
        <item name="colorOnPrimary">@color/anti_flash_white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/klein_blue</item>
        <item name="colorSecondaryVariant">@color/klein_blue</item>
        <item name="colorOnSecondary">@color/anti_flash_white</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
        <item name="android:windowBackground">@color/anti_flash_white</item>
        <item name="android:textColor">@color/better_black</item>
    </style>
</resources>